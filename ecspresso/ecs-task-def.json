{
  "containerDefinitions": [
    {
      "cpu": 0,
      "dependsOn": [
        {
          "condition": "START",
          "containerName": "rails"
        }
      ],
      "essential": true,
      "image": "826277636554.dkr.ecr.ap-northeast-1.amazonaws.com/readable-coder-prod-nginx:latest",
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/readable-coder-prod/nginx",
          "awslogs-region": "ap-northeast-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "mountPoints": [
        {
          "containerPath": "/app/tmp/sockets",
          "sourceVolume": "puma-socket"
        }
      ],
      "name": "nginx",
      "portMappings": [
        {
          "appProtocol": "",
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp"
        }
      ]
    },
    {
      "cpu": 0,
      "essential": true,
      "image": "826277636554.dkr.ecr.ap-northeast-1.amazonaws.com/readable-coder-prod-rails:latest",
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/readable-coder-prod/rails",
          "awslogs-region": "ap-northeast-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "mountPoints": [
        {
          "containerPath": "/app/tmp/sockets",
          "sourceVolume": "puma-socket"
        }
      ],
      "name": "rails",
      "secrets": [
        {
          "name": "RAILS_MASTER_KEY",
          "valueFrom": "/readable-coder/prod/RAILS_MASTER_KEY"
        }
      ]
    }
  ],
  "cpu": "256",
  "executionRoleArn": "arn:aws:iam::826277636554:role/readable-coder-prod-ecs-task-execution",
  "family": "readable-coder-prod",
  "ipcMode": "",
  "memory": "512",
  "networkMode": "awsvpc",
  "pidMode": "",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "tags": [
    {
      "key": "Name",
      "value": "readable-coder-prod"
    }
  ],
  "taskRoleArn": "arn:aws:iam::826277636554:role/readable-coder-prod-ecs-task",
  "volumes": [
    {
      "host": {},
      "name": "puma-socket"
    }
  ]
}
